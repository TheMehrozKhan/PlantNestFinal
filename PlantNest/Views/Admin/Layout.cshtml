    <script src="~/AdminTemplate/assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/AdminTemplate/assets/js/sidebarmenu.js"></script>
    <script src="~/AdminTemplate/assets/js/app.min.js"></script>
    <script src="~/AdminTemplate/assets/libs/apexcharts/dist/apexcharts.min.js"></script>
    <script src="~/AdminTemplate/assets/libs/simplebar/dist/simplebar.js"></script>
